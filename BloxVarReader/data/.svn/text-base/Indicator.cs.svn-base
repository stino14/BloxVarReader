using System;
using System.Collections.Generic;
using System.Text;

namespace BloxVarReader.data
{
	public class Indicator
	{
		private string m_szType;
		private string[] m_szParams;
		private Scope m_stScope;
		private string m_szScriptName;
		private string[] m_szSourceParameters;
		private bool m_bPlot;

		public Indicator()
		{
			m_szParams = new string[3];
			m_szSourceParameters = new string[2];
		}


		#region Properties
		public string Type
		{
			get { return m_szType; }
			set { m_szType = value; }
		}

		public string[] Params
		{
			get { return m_szParams; }
			set { m_szParams = value; }
		}

		public Scope Scope
		{
			get { return m_stScope; }
			set { m_stScope = value; }
		}

		public string ScriptName
		{
			get { return m_szScriptName; }
			set { m_szScriptName = value; }
		}

		public string[] SourceParameters
		{
			get { return m_szSourceParameters; }
			set { m_szSourceParameters = value; }
		}

		public bool Plot
		{
			get { return m_bPlot; }
			set { m_bPlot = value; }
		}
		#endregion
	}
}
